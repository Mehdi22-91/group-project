#include <iostream>
using namespace std;

int main() {
    int n;

    // Read number of elements
    cin >> n;

    int arr[n];

    // Read array elements
    for (int i = 0; i < n; i++) {
        cin >> arr[i];
    }

    // Print array in reverse order
    for (int i = n - 1; i >= 0; i--) {
        cout << arr[i] << " ";
    }

    return 0;
}
