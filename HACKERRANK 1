#include <iostream>
using namespace std;

// Function to find the maximum of four integers
int max_of_four(int a, int b, int c, int d) {
    int max = a;        // Assume first number is the largest

    if (b > max)        // Compare with second number
        max = b;

    if (c > max)        // Compare with third number
        max = c;

    if (d > max)        // Compare with fourth number
        max = d;

    return max;         // Return the greatest value
}

int main() {
    int a, b, c, d;

    // Read four integers (input handled automatically in HackerRank)
    cin >> a >> b >> c >> d;

    // Call the function and print the result
    cout << max_of_four(a, b, c, d);

    return 0;
}
